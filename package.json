  "main": "index.js",
  "scripts": {
    "start": "export ENV=dev && nodemon src/index.ts",
    "watch": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./build/server.js\"",
    "migrate:test": "db-migrate --env test up && db-migrate up",
    "migrate:dev": "db-migrate db:create storefront && db-migrate --env dev up && db-migrate up",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npx tsc",
    "jasmine": "jasmine",
    "test": "export ENV=test && npm run build && npm run jasmine"
  },
